#!/system/bin/sh

### CPU Over Volt!
echo "1350 1300 1275 1250 1225 1200 1175 1162 1150 1125 1100 1075 1050 1025 1000 975 950 900 875 850 825 800 775 750 725 700 687 675 " > /sys/devices/system/cpu/cpu0/cpufreq/UV_mV_table
#GPU Voltage Table
echo "1425 1375 1325 1275 1225 1175 1125 1075 1025 975 925" > /sys/devices/system/cpu/cpu0/cpufreq/gpu_voltage_control

